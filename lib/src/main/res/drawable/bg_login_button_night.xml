<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="50dip" />
            <solid android:color="@color/colorMapoGreenVeryDark" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="50dip" />
            <solid android:color="@color/colorMapoGreenDark" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="50dip" />
            <solid android:color="@color/colorMapoGreen" />
        </shape>
    </item>
</layer-list><!--<stroke android:width="1dip" android:color="#13b0ca" />--><!--<gradient android:angle="-90" android:startColor="#8dbab3" android:endColor="#58857e" />-->



